<h1>Listing extra_types</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @extra_types.each do |extra_type| %>
  <tr>
      <td><%= extra_type.code %></td>
      <td><%= extra_type.name %></td>
    <td><%= link_to 'Show', extra_type %></td>
    <td><%= link_to 'Edit', edit_extra_type_path(extra_type) %></td>
    <td><%= link_to 'Destroy', extra_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Extra type', new_extra_type_path %>
