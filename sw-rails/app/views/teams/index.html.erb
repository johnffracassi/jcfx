<h1>Listing Teams</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Club</th>
    <th>Competition</th>
    <th>Season</th>
    <th>Grade</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |sp| %>
  <tr>
      <td><%= sp.id %></td>
      <td><%= sp.club.name %></td>
      <td><%= sp.competition.name %></td>
      <td><%= sp.season.name %></td>
      <td><%= sp.grade.name %></td>
    <td><%= link_to 'Show', sp %></td>
    <td><%= link_to 'Edit', edit_team_path(sp) %></td>
    <td><%= link_to 'Destroy', sp, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team', new_team_path %>
